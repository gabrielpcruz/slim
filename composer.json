{
    "name": "gabrielpcruz/slim",
    "description": "Skeleton slim framework 4 application (http + cli)",
    "minimum-stability": "stable",
    "license": "proprietary",
    "authors": [
        {
            "name": "Gabriel Cruz",
            "email": "gabrielcruz.mail@gmail.com"
        }
    ],
    "require": {
        "slim/slim": "^4.10",
        "guzzlehttp/psr7": "^2.4",
        "php-di/php-di": "^6.4",
        "adbario/php-dot-notation": "^3.1",
        "slim/twig-view": "^3.3",
        "slashtrace/slashtrace": "^1.1",
        "fig/http-message-util": "^1.1",
        "illuminate/database": "^8.83",
        "symfony/console": "^5.4",
        "symfony/yaml": "^5.4",
        "ext-json": "*",
        "ext-yaml": "*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "require-dev": {
        "codeception/codeception": "^4.1",
        "codeception/module-asserts": "^1.0.0"
    }
